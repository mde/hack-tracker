<div class="hero-unit">
  <h2 class="id"><%= hack.id %></h2>
  <%- linkTo('Edit this hack', editHackPath(params.id), {class: 'btn pull-right'}); %>
</div>

<h2>Hack Properties</h2>
<% for(var i in hack) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= hack[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>